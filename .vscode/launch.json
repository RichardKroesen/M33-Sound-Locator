    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "cortex-debug",
            "request": "launch",
            "showDevDebugOutput": "raw",
            "name": "Debug J-Link",
            "cwd": "${workspaceRoot}",
            "executable": "${command:cmake.launchTargetPath}",
            "servertype": "jlink",
            "device": "RP2350_M33_0",
            "numberOfProcessors": 1,
            "targetProcessor": 0,
            "interface": "swd",
            "runToEntryPoint": "main",
            "svdFile": "${workspaceFolder}/RP2350.svd",
          }
    ]
}